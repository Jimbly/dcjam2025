---
open_vis: true
default_wall: solid
build_hide: true
visuals:
- pass: bg
  type: simple_floor
  opts:
    atlas: default
    tile: unused
- pass: bg
  type: simple_ceiling
  opts:
    atlas: default
    tile: unused

corners: # pillars and floor/ceiling details
  one:
  - pass: details
    type: simple_pillar
    opts:
      quadrants: 1
      radius: 0.125
      segments: 8
      roundness: 0
      atlas: default
      tile: unused
      height: 1
      offs: [0,0,0]
  - pass: details
    type: simple_corner_floor
    opts:
      quadrants: 1
      scale: 1
      atlas: default
      tile: unused
  two:
  - pass: celldetails
    type: simple_pillar
    opts:
      quadrants: 2
      radius: 0.125
      segments: 8
      roundness: 0
      atlas: default
      tile: unused
      height: 1
      offs: [0,0,0]
  - pass: details
    type: simple_corner_floor
    opts:
      quadrants: 2
      scale: 1
      atlas: default
      tile: unused
  three:
  - pass: details
    type: simple_pillar
    opts:
      quadrants: 3
      radius: 0.125
      segments: 8
      roundness: 0
      atlas: default
      tile: unused
      height: 1
      offs: [0,0,0]
  - pass: details
    type: simple_corner_floor
    opts:
      quadrants: 3
      scale: 1
      atlas: default
      tile: unused
