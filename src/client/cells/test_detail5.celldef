---
open_move: true
open_vis: true
build_hide: true
visuals:
- pass: bg
  type: simple_floor
  opts:
    atlas: default
    tile: unused
- pass: bg
  type: simple_ceiling
  opts:
    atlas: default
    tile: unused
- pass: celldetails
  type: simple_billboard
  opts:
    atlas: default
    tile: unused
    width: 1
    height: 1
    offs: [0, 0, 0]
    face_camera: true # Otherwise faces frustum
    do_alpha: true
