---
open_move: true
open_vis: true
visuals:
- pass: bg
  type: simple_floor
  opts:
    atlas: moon
    tile: floor
- pass: bg
  type: simple_ceiling
  opts:
    atlas: moon
    tile: ceiling
- pass: celldetails
  type: simple_billboard
  opts:
    atlas: base
    tile: detail2
    width: 0.45
    height: 0.6
    offs: [0, 0, 0]
    face_camera: false # Otherwise faces frustum
    do_alpha: true
