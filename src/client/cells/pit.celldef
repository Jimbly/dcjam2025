---
open_vis: true
ascii_char: '*'
map_view_detail_frame_visited: pit
blocks_pathing_if_visited: true
build_hide: true
visuals:
- pass: bg
  type: simple_floor
  opts:
    atlas: default
    tile: unused
- pass: bg
  type: simple_ceiling
  opts:
    atlas: whitebox
    tile: solid
visuals_visited:
- pass: bg
  type: simple_floor
  opts:
    atlas: default
    tile: unused
- pass: bg
  type: simple_ceiling
  opts:
    atlas: whitebox
    tile: solid
default_events:
- floor_pit 1